<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="VeterinaryManagerGenNHibernate"
		                   namespace="VeterinaryManagerGenNHibernate.EN.VeterinaryManager">
			<class name="ProductosEN" table="[Productos]">
						<id name="Id" column="[idProductos]"/>
					<property name="Nombre" type="string"> 
						<column name="[nombre]"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="[descripcion]"  not-null="true"/>
					</property>
					<property name="Precio" type="float"> 
						<column name="[precio]"  not-null="true"/>
					</property>
					<property name="Stock" type="integer"> 
						<column name="[stock]"  not-null="true"/>
					</property>
			
			
			
			
			
					
					
					
					
					
		
		  </class>
		  
		  <query name="ProductosENReadAllHQL">from ProductosEN</query>
		</hibernate-mapping>
	